{
  "name": "@lokalise/your-project-name",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "lint": "eslint --cache . --ext .js,.cjs,.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run",
    "prepare": "husky install",
    "commit": "commit"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/your-project-name.umd.cjs",
  "module": "./dist/your-project-name.js",
  "exports": {
    ".": {
      "import": "./dist/your-project-name.js",
      "require": "./dist/your-project-name.umd.cjs"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@commitlint/prompt-cli": "17.4.4",
    "@lokalise/eslint-config-lokalise-frontend": "0.3.1",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "prettier": "2.8.4",
    "semantic-release": "20.1.0",
    "vite": "4.1.4",
    "vitest": "0.28.5"
  }
}
